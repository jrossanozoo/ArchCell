	function Cuentaecommerce_Access() as variant
		if this.ldestroy
		else
			if This.lInstanciarSubEntidadaDemanda
				if ( !vartype( this.Cuentaecommerce ) = 'O' or isnull( this.Cuentaecommerce ) )
					this.Cuentaecommerce = _Screen.zoo.instanciarentidad( 'Ecommerce' )
					this.Cuentaecommerce.lEsSubEntidad = .t.
					this.enlazar( 'Cuentaecommerce.EventoObtenerLogueo', 'inyectarLogueo' )
					this.enlazar( 'Cuentaecommerce.EventoObtenerInformacion', 'inyectarInformacion' )
				endif
				if !this.lBuscandoCodigo
					if this.Cuentaecommerce.CODIGO # this.Cuentaecommerce_PK
						this.Cuentaecommerce.CODIGO = this.Cuentaecommerce_PK
					endif
				endif
			endif
		endif
		return this.Cuentaecommerce
	endfunc

