		lnParametros = pcount()lcParametros = ''		lnIdClase = _screen.Mocks.BuscarMock( 'ECOMMERCE' )		if lnIdClase > 0			lnIdMetodo =  _screen.Mocks.BuscarMetodo( lnIdClase, 'valorctacte_access', lcParametros )			if lnIdMetodo > 0				return this.ObtenerResultado( 'valorctacte_access', lcParametros )			endif		endif		if this.ldestroy		else			if This.lInstanciarSubEntidadaDemanda				if ( !vartype( this.Valorctacte ) = 'O' or isnull( this.Valorctacte ) )					this.Valorctacte = _Screen.zoo.instanciarentidad( 'Valor' )					this.Valorctacte.lEsSubEntidad = .t.					this.enlazar( 'Valorctacte.EventoObtenerLogueo', 'inyectarLogueo' )					this.enlazar( 'Valorctacte.EventoObtenerInformacion', 'inyectarInformacion' )				endif				if !this.lBuscandoCodigo					if this.Valorctacte.CODIGO # this.Valorctacte_PK						this.Valorctacte.CODIGO = this.Valorctacte_PK					endif				endif			endif		endif		return this.Valorctacte
	EndFunc

	*-----------------------------------------------------------------------------------------
	function valorctacte_pk_assign( txval as variant ) as void                                                                                                                                                                                                
		local lcParametros as string, lcParam as string, lni as integer, lnParametros as integer, ;		lnIdMetodo as Integer, lnIdClase as Integer, lxParam1lnParametros = pcount()lxParam1 = txvallcParametros = ''		for lni = 1 to lnParametros			lcParam = 'lxParam' + alltrim(str(lni))			lcParametros = lcParametros + iif( !empty(lcParametros),',','') + iif( vartype( &lcParam ) = 'O' or isnull(&lcParam), "'*OBJETO'", goLibrerias.ValorAString( &lcParam ) )		endfor		lnIdClase = _screen.Mocks.BuscarMock( 'ECOMMERCE' )		if lnIdClase > 0			lnIdMetodo =  _screen.Mocks.BuscarMetodo( lnIdClase, 'valorctacte_pk_assign', lcParametros )			if lnIdMetodo > 0				return this.ObtenerResultado( 'valorctacte_pk_assign', lcParametros )			endif		endif		dodefault( txVal )		loEntidadValor = _screen.zoo.instanciarentidad( "VALOR" )		loEntidadValor.Codigo = txVal		if ( txVal != "" and loEntidadValor.Tipo != 6 )			this.Setear_ValorCtaCte( "" )			this.ValorCtaCte_PK = ""			goMensajes.enviar( "Debe ingresar un valor de tipo cuenta corriente.", 0, 2 )		endif		loEntidadValor.Release()
	EndFunc

