				.Usuariomodificacionfw = []
				.Estadotransferencia = []
				.Versionaltafw = []
				.Zadsfw = []
				.Descripcionfw = []
				.Versionmodificacionfw = []
				.Horamodificacionfw = []
				.Basededatosmodificacionfw = []
