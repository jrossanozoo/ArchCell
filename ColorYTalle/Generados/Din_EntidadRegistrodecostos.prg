			llRetorno = .ValidarTalle() and llRetorno
			llRetorno = .ValidarCantidad() and llRetorno
			llRetorno = .ValidarStock() and llRetorno
			llRetorno = .ValidarPrecio() and llRetorno
			llRetorno = .ValidarEjercicio() and llRetorno
			llRetorno = .ValidarBasededatos() and llRetorno
			llRetorno = .ValidacionAtributos() and llRetorno
			llRetorno = .ValidacionDetalles() and llRetorno
			if .VerificarContexto( 'CB' )
			else
				llRetorno = .ValidacionTimestamp() and llRetorno
			endif
		EndWith

		If Empty( this.IDENTIFICADOR )
			llRetorno = .F.
			This.AgregarInformacion( 'No se puede grabar. Código Vacío', 0 )
		Endif
		if This.ValidarIngreso( transform( this.IDENTIFICADOR ) )
		else
			llRetorno = .F.
			this.AgregarInformacion( 'Caracteres Inválidos en el código', 0 )
		EndIf
