	*--------------------------------------------------------------------------------------------------------
	function Equivalenciaafip_Access() as variant
		if this.ldestroy
		else
			if This.lInstanciarSubEntidadaDemanda
				if ( !vartype( this.Equivalenciaafip ) = 'O' or isnull( this.Equivalenciaafip ) )
					this.Equivalenciaafip = _Screen.zoo.instanciarentidad( 'Monedaafip' )
					this.Equivalenciaafip.lEsSubEntidad = .t.
					this.enlazar( 'Equivalenciaafip.EventoObtenerLogueo', 'inyectarLogueo' )
					this.enlazar( 'Equivalenciaafip.EventoObtenerInformacion', 'inyectarInformacion' )
				endif
				if !this.lBuscandoCodigo
					if this.Equivalenciaafip.CODIGO # this.Equivalenciaafip_PK
						this.Equivalenciaafip.CODIGO = this.Equivalenciaafip_PK
					endif
				endif
			endif
		endif
		return this.Equivalenciaafip
	endfunc

