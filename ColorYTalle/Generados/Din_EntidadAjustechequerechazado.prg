	*--------------------------------------------------------------------------------------------------------
	function Limpiar( tlForzar as boolean ) as void
		local loError as Exception
		with this
			Try
				.lLimpiando = .t.
				dodefault( tlForzar )
				.LimpiarFlag()

				.Fechaimpo = ctod( '  /  /    ' )
				.Fechatransferencia = ctod( '  /  /    ' )
				.Fechamodificacionfw = ctod( '  /  /    ' )
				.Fechaaltafw = ctod( '  /  /    ' )
				.Fechaexpo = ctod( '  /  /    ' )
				.Usuarioaltafw = []
				.Usuariomodificacionfw = []
				.Seriemodificacionfw = []
				.Horaaltafw = []
				.Seriealtafw = []
				.Horaimpo = []
				.Horaexpo = []
				.Versionaltafw = []
				.Basededatosmodificacionfw = []
				.Estadotransferencia = []
				.Horamodificacionfw = []
				.Zadsfw = []
				.Versionmodificacionfw = []
				.Basededatosaltafw = []
				.Tipoagrupamientopublicaciones = 0
				.Timestamp = 0
				.Codigo = []
				.Nombre = []
				.Agrupublidetalle.Limpiar()
				.Realizaajuste = 0
				.Motivoajuste_PK = []
				.Realizarecargo = 0
				.Tiporecargo = 0
				.Porcentajerecargo = 0
				.Importerecargo = 0
				.Tipocomprobante = 0
				.Articulorecargo_PK = []
				.Valorrecargo_PK = []
				.Observacion = []
				.LimpiarAtributosVirtuales()
				.oAD.Limpiar()
			catch to loError
				goServicios.Errores.LevantarExcepcion( loError )
			finally
				.lLimpiando = .f.
				if tlForzar
				else
					.SetearValoresSugeridos()
				endif
			EndTry
		endwith
	endfunc
	*--------------------------------------------------------------------------------------------------------
	Function SetearValoresSugeridos() as Void
		With This
			.lEstaSeteandoValorSugerido = .T.
			if this.EsNuevo() and this.DebeSugerirCodigo() and this.EstaEnProceso()
				this.SetearCodigoSugerido()
