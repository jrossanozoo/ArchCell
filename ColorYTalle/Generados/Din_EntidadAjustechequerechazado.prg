	*--------------------------------------------------------------------------------------------------------
	function Motivoajuste_Access() as variant
		if this.ldestroy
		else
			if This.lInstanciarSubEntidadaDemanda
				if ( !vartype( this.Motivoajuste ) = 'O' or isnull( this.Motivoajuste ) )
					this.Motivoajuste = _Screen.zoo.instanciarentidad( 'Motivo' )
					this.Motivoajuste.lEsSubEntidad = .t.
					this.enlazar( 'Motivoajuste.EventoObtenerLogueo', 'inyectarLogueo' )
					this.enlazar( 'Motivoajuste.EventoObtenerInformacion', 'inyectarInformacion' )
				endif
				if !this.lBuscandoCodigo
					if this.Motivoajuste.CODIGO # this.Motivoajuste_PK
						this.Motivoajuste.CODIGO = this.Motivoajuste_PK
					endif
				endif
			endif
		endif
		return this.Motivoajuste
	endfunc

