	function Limpiar( tlForzar as boolean ) as void
		local loError as Exception
		with this
			Try
				.lLimpiando = .t.
				dodefault( tlForzar )
				.LimpiarFlag()

				.Comportamiento = 0
				.Noprocesarstock = .F.
				.Equivalencia_PK = []
				.Codigo = []
				.Inventario_PK = []
