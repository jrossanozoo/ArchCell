		if This.CargaManual()
			if this.Validar_Cantidadinsumo( lxVal )
				this.EventoAntesDeSetear( This, 'Cantidadinsumo', lxValOld, lxVal  )
				this.Setear_Cantidadinsumo( lxVal )
				this.EventoDespuesDeSetear( This, 'Cantidadinsumo', lxValOld, lxVal  )
				If Not (lxVal == lxValOld) And This.PuedeEjecutarCambioSumarizado()
					this.AcumularSumarizado( .t., 'Cantidadinsumo', lxVal, lxValOld)
				EndIf
				If lxValOld != lxVal
					If This.PuedeEjecutarHaCambiado()
						this.HaCambiado( 'Cantidadinsumo', This )
					Endif
				EndIf
			EndIf
		Else
			This.Setear_Cantidadinsumo( lxVal ) 
		EndIf
		dodefault( lxVal )

	endfunc

