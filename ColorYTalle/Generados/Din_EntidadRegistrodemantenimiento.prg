				.Usuarioaltafw = []
				.Versionmodificacionfw = []
				.Zadsfw = []
				.Versionaltafw = []
				.Usuariomodificacionfw = []
				.Seriealtafw = []
				.Seriemodificacionfw = []
				.Horamodificacionfw = []
				.Estadotransferencia = []
				.Sitio = []
				.Basededatosmodificacionfw = []
				.Estado = 0
