		<connectionString value="[{STRINGCONNECTIONSQLSERVER}]"/>
		<commandText value="INSERT INTO [|@_screen.zoo.app.cSchemaDefault@|].[Logueos] ([Fecha] ,[Nivel] ,[Accion] ,[Logger] ,[BaseDeDatos] ,[EstadoDelSistema] ,[Aplicacion] ,[Version] ,[Serie] ,[Usuario] ,[NombrePc] ,[UsuarioPc] ,[OrigenLogueo] ,[Mensaje] ,[Excepcion]) VALUES (@fecha ,@nivel ,@accion ,@logger ,@basedatos ,@estadosistema ,@aplicacion ,@version ,@serie ,@usuario ,@nombrepc ,@usuariopc ,@origenlogueo ,@mensaje ,@excepcion ) "/>
		<parameter>
			<parameterName value="@fecha"/>
			<dbType value="Datetime"/>
			<layout type="log4net.Layout.RawTimeStampLayout"/>
		</parameter>
		<parameter>
			<parameterName value="@nivel"/>
			<dbType value="String"/>
			<Size value="50"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%level"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@accion"/>
			<dbType value="String"/>
			<Size value="255"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{Accion}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@logger"/>
			<dbType value="String"/>
			<Size value="255"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%logger"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@basedatos"/>
			<dbType value="String"/>
			<Size value="50"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{BaseDatos}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@estadosistema"/>
			<dbType value="Int32"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{EstadoSistema}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@aplicacion"/>
			<dbType value="String"/>
			<Size value="100"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{Aplicacion}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@version"/>
			<dbType value="String"/>
			<Size value="20"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{Version}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@serie"/>
			<dbType value="String"/>
			<Size value="10"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{Serie}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@usuario"/>
			<dbType value="String"/>
			<Size value="50"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{Usuario}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@nombrepc"/>
			<dbType value="String"/>
			<Size value="100"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{NombrePc}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@usuariopc"/>
			<dbType value="String"/>
			<Size value="100"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{UsuarioPc}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@origenlogueo"/>
			<dbType value="String"/>
			<Size value="20"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%property{OrigenLogueo}"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@mensaje"/>
			<dbType value="String"/>
			<Size value="4000"/>
			<layout type="log4net.Layout.PatternLayout">
				<conversionPattern value="%message"/>
			</layout>
		</parameter>
		<parameter>
			<parameterName value="@excepcion"/>
			<dbType value="String"/>
			<Size value="2000"/>
			<layout type="log4net.Layout.ExceptionLayout"/>
		</parameter>
	</appender>
