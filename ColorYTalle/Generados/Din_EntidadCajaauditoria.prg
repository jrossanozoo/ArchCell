				.Detallevaloresauditoria.Limpiar()
				.Seriealtafw = []
				.Seriemodificacionfw = []
				.Usuariomodificacionfw = []
				.Versionmodificacionfw = []
				.Zadsfw = []
				.Versionaltafw = []
				.Usuarioaltafw = []
				.Horamodificacionfw = []
