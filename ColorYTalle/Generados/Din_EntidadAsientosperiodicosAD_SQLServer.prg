	Function Eliminar() As Boolean
		local loError as Exception, loEx as Exception, lnI as Integer, loSentenciasTriggers as zoocoleccion OF zoocoleccion.prg
		with this.oEntidad
			local  lxAsiperFecexpo, lxAsiperFaltafw, lxAsiperFmodifw, lxAsiperFectrans, lxAsiperFecimpo, lxAsiperSmodifw, lxAsiperHmodifw, lxAsiperSaltafw, lxAsiperUaltafw, lxAsiperVmodifw, lxAsiperZadsfw, lxAsiperValtafw, lxAsiperHoraimpo, lxAsiperUmodifw, lxAsiperHaltafw, lxAsiperBdaltafw, lxAsiperBdmodifw, lxAsiperEsttrans, lxAsiperHoraexpo, lxAsiperCodigo
				lxAsiperFecexpo = ctod( '  /  /    ' )			lxAsiperFaltafw = ctod( '  /  /    ' )			lxAsiperFmodifw = ctod( '  /  /    ' )			lxAsiperFectrans = ctod( '  /  /    ' )			lxAsiperFecimpo = ctod( '  /  /    ' )			lxAsiperSmodifw = []			lxAsiperHmodifw = []			lxAsiperSaltafw = []			lxAsiperUaltafw = []			lxAsiperVmodifw = []			lxAsiperZadsfw = []			lxAsiperValtafw = []			lxAsiperHoraimpo = []			lxAsiperUmodifw = []			lxAsiperHaltafw = []			lxAsiperBdaltafw = []			lxAsiperBdmodifw = []			lxAsiperEsttrans = []			lxAsiperHoraexpo = []			lxAsiperCodigo = []
		endwith
		
		with this
		Try
			this.SetearConexionParaTransaccion()
			this.oConexion.EjecutarSql( 'BEGIN TRANSACTION' )
			this.oConexion.EjecutarSql( [delete from ZooLogic.ASIPER where "Codigo" = ] + "'" + this.FormatearTextoSql( .oEntidad.CODIGO ) + "'"+ goServicios.RealTime.ObtenerTagEstimulo( .oEntidad.cNombre ) )
			.GenerarSentenciasComponentes()
