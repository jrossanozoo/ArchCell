	*--------------------------------------------------------------------------------------------------------
	function Inicializar() as boolean
		dodefault()
		this.oColeccionVSFW = _screen.zoo.CrearObjeto( 'zoocoleccion' )
		this.oColeccionVS = _screen.zoo.CrearObjeto( 'zoocoleccion' )
		oICol = goServicios.SaltosDeCampoYValoresSugeridos.ObtenerValorSugeridoDeUnaEntidadDetalle( 'MAQUINARIA', 'Consumos')
		for each oItmUs in oICol
			this.oColeccionVS.Agregar( '.ValorSugerido' + alltrim(oItmUs.atributo) + '()' )
			try
				this.oColeccionVSFW.Quitar( alltrim( proper( oItmUs.atributo ) ) )
			catch to loError
			endtry
		endfor
	endfunc

	*--------------------------------------------------------------------------------------------------------
	function Consumo_PK_Assign( txVal as variant ) as void

		Local lxValOld As Variant
		lxValOld = this.Consumo_PK
		local lxVal as Variant
		local loSugerido as Object
		
		lxVal = txVal
		lxVal = iif( len( lxVal ) < 15, padr( lxVal, 15 ), lxVal )
		this.Consumo_PK = lxVal

		if this.Validar_Consumo( lxVal, lxValOld )
			this.EventoAntesDeSetear( This, 'Consumo_Pk', lxValOld, lxVal  )
			This.Setear_Consumo( lxVal )
			this.EventoDespuesDeSetear( This, 'Consumo_Pk', lxValOld, lxVal  )
			If lxValOld != lxVal or empty( this.Consumodetalle ) 
				this.Consumodetalle = This.Consumo.Descripcion
			endif
			If lxValOld != lxVal
				If This.PuedeEjecutarHaCambiado()
					this.HaCambiado( 'Consumo_Pk', This )
				Endif
			EndIf
		endif
		dodefault( lxVal )

	endfunc

