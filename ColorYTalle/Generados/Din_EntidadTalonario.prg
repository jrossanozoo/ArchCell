	function Talonariorela_Access() as variant
		if this.ldestroy
		else
			if This.lInstanciarSubEntidadaDemanda
				if ( !vartype( this.Talonariorela ) = 'O' or isnull( this.Talonariorela ) )
					this.Talonariorela = _Screen.zoo.instanciarentidad( 'Talonario' )
					this.Talonariorela.lEsSubEntidad = .t.
					this.enlazar( 'Talonariorela.EventoObtenerLogueo', 'inyectarLogueo' )
					this.enlazar( 'Talonariorela.EventoObtenerInformacion', 'inyectarInformacion' )
				endif
				if !this.lBuscandoCodigo
					if this.Talonariorela.CODIGO # this.Talonariorela_PK
						this.Talonariorela.CODIGO = this.Talonariorela_PK
					endif
				endif
			endif
		endif
		return this.Talonariorela
	endfunc

