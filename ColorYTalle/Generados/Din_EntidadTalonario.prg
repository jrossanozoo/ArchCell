				.Seriemodificacionfw = []
				.Usuarioaltafw = []
				.Horamodificacionfw = []
				.Estadotransferencia = []
				.Formula = []
				.Basededatosaltafw = []
				.Basededatosmodificacionfw = []
				.Timestamp = 0
