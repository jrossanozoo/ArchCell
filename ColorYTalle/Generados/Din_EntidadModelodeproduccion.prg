				if ( !vartype( this.Productofinal ) = 'O' or isnull( this.Productofinal ) )
					this.Productofinal = _Screen.zoo.instanciarentidad( 'Articulo' )
					this.Productofinal.lEsSubEntidad = .t.
					this.enlazar( 'Productofinal.EventoObtenerLogueo', 'inyectarLogueo' )
					this.enlazar( 'Productofinal.EventoObtenerInformacion', 'inyectarInformacion' )
				endif
				if !this.lBuscandoCodigo
					if this.Productofinal.CODIGO # this.Productofinal_PK
						this.Productofinal.CODIGO = this.Productofinal_PK
					endif
				endif
			endif
		endif
		return this.Productofinal
	endfunc

