				.Timestamp = 0
				.Zadsfw = []
				.Seriealtafw = []
				.Usuarioaltafw = []
				.Horamodificacionfw = []
				.Versionaltafw = []
				.Usuariomodificacionfw = []
				.Versionmodificacionfw = []
				.Seriemodificacionfw = []
				.Estadotransferencia = []
				.Horaimpo = []
				.Horaexpo = []
				.Horaaltafw = []
				.Basededatosmodificacionfw = []
				.Basededatosaltafw = []
				.Observacion = []
				.Id = 0
				.Descripcion = []
				.Descripcionestado = []
