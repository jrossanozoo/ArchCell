	function ValidarEquipoPOS() as boolean
		local llRetorno as boolean
		llRetorno = dodefault()
		if empty( this.EquipoPOS_PK )
		else
			If this.EquipoPOS.oAD.ConsultarPorClavePrimaria()
			Else
				this.AgregarInformacion( 'El dato buscado ' + alltrim(transform( this.EquipoPOS_PK ))+ ' de la entidad ' +  upper( this.EquipoPOS.cDescripcion ) + ' no existe.')
				llRetorno = .F.
