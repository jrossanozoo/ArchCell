		this.oColeccionVSFW.agregar('.ValorSugeridoFWFechaalta()','Fechaalta')
		this.oColeccionVSFW.agregar('.ValorSugeridoFWFechacierredelote()','Fechacierredelote')
		this.oColeccionVSFW.agregar('.ValorSugeridoFWHoraalta()','Horaalta')
		this.oColeccionVSFW.agregar('.ValorSugeridoFWSerieorigen()','Serieorigen')
		this.oColeccionVSFW.agregar('.ValorSugeridoFWEquipopos()','Equipopos')
		oICol = goServicios.SaltosDeCampoYValoresSugeridos.ObtenerValorSugeridoDeUnaEntidadDetalle( 'CIERREDELOTE', '')
		for each oItmUs in oICol
			this.oColeccionVS.Agregar( '.ValorSugerido' + alltrim(oItmUs.atributo) + '()' )
			try
				this.oColeccionVSFW.Quitar( alltrim( proper( oItmUs.atributo ) ) )
			catch to loError
			endtry
		endfor
	endfunc

