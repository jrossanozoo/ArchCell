	function Equipopos_Access() as variant
		if this.ldestroy
		else
			if This.lInstanciarSubEntidadaDemanda
				if ( !vartype( this.Equipopos ) = 'O' or isnull( this.Equipopos ) )
					this.Equipopos = _Screen.zoo.instanciarentidad( 'Pos' )
					this.Equipopos.lEsSubEntidad = .t.
					this.enlazar( 'Equipopos.EventoObtenerLogueo', 'inyectarLogueo' )
					this.enlazar( 'Equipopos.EventoObtenerInformacion', 'inyectarInformacion' )
				endif
				if !this.lBuscandoCodigo
					if this.Equipopos.CODIGO # this.Equipopos_PK
						this.Equipopos.CODIGO = this.Equipopos_PK
					endif
				endif
			endif
		endif
		return this.Equipopos
	endfunc

