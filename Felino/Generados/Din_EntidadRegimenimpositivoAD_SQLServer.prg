		with this.oEntidad
			local  lxRegimpFecimpo, lxRegimpFectrans, lxRegimpFmodifw, lxRegimpFecexpo, lxRegimpFaltafw, lxRegimpTimestamp, lxRegimpBloqreg, lxRegimpUaltafw, lxRegimpSmodifw, lxRegimpUmodifw, lxRegimpValtafw, lxRegimpVmodifw, lxRegimpSaltafw, lxRegimpZadsfw, lxRegimpHaltafw, lxRegimpBdmodifw, lxRegimpBdaltafw, lxRegimpHmodifw, lxRegimpEsttrans, lxRegimpHoraimpo, lxRegimpHoraexpo, lxRegimpObs, lxRegimpCodigo, lxRegimpDescrip, lxRegimpTipo
				lxRegimpFecimpo = ctod( '  /  /    ' )			lxRegimpFectrans = ctod( '  /  /    ' )			lxRegimpFmodifw = ctod( '  /  /    ' )			lxRegimpFecexpo = ctod( '  /  /    ' )			lxRegimpFaltafw = ctod( '  /  /    ' )			lxRegimpTimestamp = goLibrerias.ObtenerTimestamp()			lxRegimpBloqreg = .F.			lxRegimpUaltafw = []			lxRegimpSmodifw = []			lxRegimpUmodifw = []			lxRegimpValtafw = []			lxRegimpVmodifw = []			lxRegimpSaltafw = []			lxRegimpZadsfw = []			lxRegimpHaltafw = []			lxRegimpBdmodifw = []			lxRegimpBdaltafw = []			lxRegimpHmodifw = []			lxRegimpEsttrans = []			lxRegimpHoraimpo = []			lxRegimpHoraexpo = []			lxRegimpObs = []			lxRegimpCodigo = []			lxRegimpDescrip = []			lxRegimpTipo = []
		endwith
		
		with this
