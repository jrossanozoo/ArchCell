	Function EsAliasDeProceso( tcAlias As String, tcPrefijo as string ) As Boolean
		local llRetorno as Boolean, lcAlias as String, lcPrefijo As String
		llRetorno = .F.
		lcAlias = upper( alltrim( tcAlias ) )
		lcPrefijo = upper( alltrim( tcPrefijo ) )
		Do Case
			Case  lcAlias == lcPrefijo + 'REGIMENIMPOSITIVO'
				llRetorno = .T.
			Case  lcAlias == lcPrefijo + 'REGIMENIMPOSITIVO_ZADSFW'
				llRetorno = .T.
			Case  lcAlias == lcPrefijo + 'REGIMENIMPOSITIVO_OBS'
				llRetorno = .T.
		EndCase
		Return llRetorno
	EndFunc

