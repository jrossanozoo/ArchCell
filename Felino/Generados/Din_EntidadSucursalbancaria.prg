				.Horamodificacionfw = []
				.Horaimpo = []
				.Usuarioaltafw = []
				.Seriealtafw = []
				.Versionaltafw = []
				.Usuariomodificacionfw = []
				.Seriemodificacionfw = []
				.Versionmodificacionfw = []
				.Basededatosmodificacionfw = []
				.Estadotransferencia = []
				.Basededatosaltafw = []
				.Codigo = []
				.Numero = 0
				.Descripcion = []
				.Piso = []
				.Entidadfinanciera_PK = []
				.Departamento = []
				.Numerosuc = 0
				.Localidad = []
				.Codigopostal = []
				.Provincia_PK = []
				.Telefono = []
				.Pais_PK = []
				.Email = []
				.Contacto = []
				.Observacion = []
				.LimpiarAtributosVirtuales()
				.oAD.Limpiar()
			catch to loError
				goServicios.Errores.LevantarExcepcion( loError )
			finally
				.lLimpiando = .f.
				if tlForzar
				else
					.SetearValoresSugeridos()
