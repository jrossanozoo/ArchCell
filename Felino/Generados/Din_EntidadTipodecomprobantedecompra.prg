	function Despacho_Access() as variant
		if this.ldestroy
		else
			if This.lInstanciarSubEntidadaDemanda
				if ( !vartype( this.Despacho ) = 'O' or isnull( this.Despacho ) )
					this.Despacho = _Screen.zoo.instanciarentidad( 'Despachodeimportacion' )
					this.Despacho.lEsSubEntidad = .t.
					this.enlazar( 'Despacho.EventoObtenerLogueo', 'inyectarLogueo' )
					this.enlazar( 'Despacho.EventoObtenerInformacion', 'inyectarInformacion' )
				endif
				if !this.lBuscandoCodigo
					if this.Despacho.DESPACHO # this.Despacho_PK
						this.Despacho.DESPACHO = this.Despacho_PK
					endif
				endif
			endif
		endif
		return this.Despacho
	endfunc

