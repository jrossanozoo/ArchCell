				if ( !vartype( this.Cajaactiva ) = 'O' or isnull( this.Cajaactiva ) )
					this.Cajaactiva = _Screen.zoo.instanciarentidad( 'Cajaestado' )
					this.Cajaactiva.lEsSubEntidad = .t.
